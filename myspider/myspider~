import sys
import pymyspider

def main ():
    argv = len (sys.argv)   
    if argv == 1:
      print "Indicar URL"
      return
    
    url = sys.argv[1]
    
    enlaces = pymyspider.retrieve_url (url)
    for e in enlaces:
      print e
    
#    if texto != -1:
#      print texto
#    else:
#      print "Error al leer la URL"
    
    
if __name__ == '__main__':
  main ()
